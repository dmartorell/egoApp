{
  "name": "@egoapp/frontend",
  "version": "0.1.0",
  "private": true,
  "description": "Frontend React application for EgoApp",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint 'src/**/*.{js,ts,jsx,tsx}' --max-warnings 0",
    "lint:check": "eslint 'src/**/*.{js,ts,jsx,tsx}' --max-warnings 0",
    "lint:fix": "eslint 'src/**/*.{js,ts,jsx,tsx}' --fix",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "clean": "rm -rf dist .turbo node_modules/.cache"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vitest": "^1.1.0"
  },
  "engines": {
    "node": ">=20.0.0",
    "yarn": ">=1.22.0"
  }
}
